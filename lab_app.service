[Unit]
Description=uWSGI Emperor
After=syslog.target

[Service]
ExecStart=/home/pi/.local/bin/uwsgi --ini /home/pi/Desktop/Bingo/lab_app.uwsgi.ini
# Requires systemd version 211 or newer
RuntimeDirectory=uwsgi
Restart=always
KillSignal=SIGQUIT
Type=notify
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target
